{
  "name": "bcc-data",
  "version": "1.0.0",
  "description": "Evolução do BCC em gráficos",
  "homepage": "http://akafts.github.io/bcc-data",
  "private": true,
  "scripts": {
    "start": "parcel ./src/index.html",
    "lint": "eslint src --ext js",
    "build": "parcel build ./src/index.html",
