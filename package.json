{
  "name": "bcc-data",
  "version": "1.0.0",
  "description": "Evolução do BCC em gráficos",
  "homepage": "http://akafts.github.io/bcc-data",
  "private": true,
  "scripts": {
    "start": "parcel ./src/index.html",
    "lint": "eslint src --ext .js,.tsx ; exit 0",
    "precommit": "npm run lint",
    "prebuild": "rm -rf dist/",
    "build": "parcel build ./src/index.html --no-source-maps",
    "postbuild": "cp -r public/* dist/",
    "build:deploy": "npm run build -- --public-url '/bcc-data/'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "chart.js": "^2.7.2",
    "d3-scale": "^2.1.2",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.5.2",
    "react-responsive-modal": "^3.4.0",
    "react-router": "^6.0.0-alpha.5",
    "react-router-dom": "^6.0.0-alpha.5",
    "react-simple-maps": "^2.1.2",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "parcel": "^2.0.0-beta.1",
    "typescript": "^3.9.5"
  },
  "author": "Gustavo Silva",
  "license": "MIT",
  "repository": ""
}
